{
  "name": "MarkdownEditor",
  "version": "1.0.0",
  "description": "Online markdown editor intended for OpenDoc users.",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm run bundleone && npm run bundletwo",
    "bundleone": "browserify lib/import.js --standalone bundleone > lib/importbundle.js",
    "bundletwo": "browserify lib/export.js --standalone bundletwo > lib/exportbundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opendocsg/editor.git"
  },
  "keywords": [
    "editor.md",
    "markdown",
    "editor"
  ],
  "author": "Pandao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pandao/editor.md/issues"
  },
  "homepage": "https://github.com/pandao/editor.md",
  "devDependencies": {
    "dateformatter": "^0.1.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.4.2",
    "gulp-header": "^1.2.2",
    "gulp-jshint": "^2.1.0",
    "gulp-minify-css": "^0.4.4",
    "gulp-notify": "^2.1.0",
    "gulp-rename": "^1.2.0",
    "gulp-replace": "^0.5.3",
    "gulp-ruby-sass": "^4.0.0",
    "gulp-uglifyjs": "^0.6.1",
    "gulp-util": "^3.0.1"
  },
  "dependencies": {
    "file-saver": "^2.0.0",
    "jszip": "^3.1.5",
    "mammoth": "^1.4.7",
    "turndown": "^5.0.3"
  }
}
